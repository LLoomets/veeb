
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>Liisi Loomets, veebiprogrammeerimine 2023</title>
	<link rel="stylesheet" href="/style/gallery.css">
    <style>
        body {
			background-color:gainsboro;
		}
    </style>
</head>
<body>
	<img src="/banner/banner.png" alt="veebiprogrammeerimise kursuse banner">
	<h1>Fotogalerii</h1>
	<ul>
		<li><a href="/">Avalehele</a></li>
	</ul>
	<hr>
	<h2>Galerii</h2>
	<div class="gallery">
	
		<% photos.forEach(function(photo) { %>
			<div class="thumbgallery">
				<img src="gallery/thumbs/<%= photo.filename %>" alt="<%= photo.alttext %>" data-filename="<%= photo.filename %>" data-id="1" class="thumbs">
				<p></p> 
			</div>
		<% }); %>

	</div>
</body>
</html>